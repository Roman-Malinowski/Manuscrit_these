	pages = {383--390},
	file = {Sarrazin et al. - 2021 - AMBIGUITY CONCEPT IN STEREO MATCHING PIPELINE.pdf:C\:\\Users\\malinoro\\Zotero\\storage\\NSB68MAB\\Sarrazin et al. - 2021 - AMBIGUITY CONCEPT IN STEREO MATCHING PIPELINE.pdf:application/pdf},
