	journal = {The Cryosphere},
	author = {Berthier, Etienne and Vincent, Christian and Magn{\'u}sson, Eyj{\'o}lfur and Gunnlaugsson, {\'A}g{\'u}st {\TH}{\'o}r and Pitte, Pierre and Le Meur, Emmanuel and Masiokas, Mariano and Ruiz, Lucas and P{\'a}lsson, Finnur and Belart, Joaqu{\'i}n M.C. and Wagnon, Patrick},
	month = dec,
