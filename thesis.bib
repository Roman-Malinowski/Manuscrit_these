@article{rieg_pleiades_2018,
	title = {Pl{\'e}iades {Tri}-{Stereo} {Data} for {Glacier} {Investigations}{\textemdash}{Examples} from the {European} {Alps} and the {Khumbu} {Himal}},
	volume = {10},
	issn = {2072-4292},
